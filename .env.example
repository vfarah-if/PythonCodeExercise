# AWS SSO Configuration
# Copy this file to .env and fill in your actual values

# AWS SSO portal URL (required)
AWS_SSO_START_URL=https://<YOUR-ONE>.awsapps.com/start/#

# AWS region for SSO (default: eu-west-2)
AWS_SSO_REGION=eu-west-2

# Default AWS account ID (optional)
AWS_SSO_ACCOUNT_ID=<YOUR-ONE>

# Default AWS region for operations
AWS_DEFAULT_REGION=eu-west-2

# Optional: Browser automation settings (for fallback mode)
# Supported browsers: chrome, firefox, safari
AWS_CREDENTIAL_BROWSER=chrome

# Run browser in headless mode (true/false)
AWS_CREDENTIAL_HEADLESS=false